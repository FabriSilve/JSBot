{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "tsc": "tsc",
    "coveralls": "coveralls",
    "jest": "jest",
    "start": "node dist/index.js",
    "postinstall": "npm run build",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "sendCoverageReport": "npm run jest -- --coverage --collectCoverageFrom=**/*.js && npm run coveralls < ./coverage/lcov.info && rm -rf ./coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.6.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/mongoose": "^5.5.6",
    "@types/node": "^12.0.8",
    "coveralls": "^3.0.4",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ]
  }
}
